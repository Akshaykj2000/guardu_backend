const mongoose=require("mongoose")

const hodSchema=new mongoose.Schema(
    {
        hodname:{
            type:String,
            required:true
        },

        department:{
            type:String,
            required:true
        },

        gender:{
            type:String,
            required:true
        },
        dateofbirth:{
            type:String,
            required:true
        },
        age:{
            type:String,
            required:true
        },
        phone:{
            type:String,
            required:true
        },
        status:{
            type:String,
        },
        email:{
            type:String,
            required:true
        },
        hodpassword:{
            type:String,
            required:true
        },
        }

    
    
)

module.exports=mongoose.model("hod",hodSchema)